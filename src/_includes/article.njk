---
layout: 'base.njk'
permalink: "/{{ title | slugify }}/"
---

<article class="main-article">
<div class="container flow">
  
  <figure class="main-article__figure">
    {% Image "./src/"+image, imageAlt  %}
  </figure>

  <section class="article-intro">
    <h1 class="article-title">
      {{ title }}
    </h1>
    <p>
      {{ description }}
    </p>
    <div class="intro-details">
      <h2 id="disc">
        {{ disc }}
      </h2>
      <p>
        {{ collabs }}
      </p>
    </div>
  </section>

  {{ content | safe }}

    Images:
    <br>
    {%- for theImage in images-%}
    the image's source: {{theImage.src}}
    <br>
    {%- endfor -%}

    Images2:
    <br>
    {%- for theImage2 in images2 -%}
      the image row margins: {{theImage2.imgRow.mSize}}
      <br>
      {%- for eachImage in theImage2.imgRow.images -%}
      the image alt: {{eachImage.alt2}}
      <br>
      {%- endfor -%}
    {%- endfor -%}

</div>
</article>
